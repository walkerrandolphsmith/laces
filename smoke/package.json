{
  "private": true,
  "name": "@laces/smoke-tests",
  "version": "1.1.0",
  "description": "Smoke tests for laces",
  "main": "index.js",
  "scripts": {
    "test": "ava"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/walkerrandolphsmith/laces.git"
  },
  "keywords": [],
  "author": "walkerrandolphsmith@gmail.com",
  "contributors": [
    "walkerrandolphsmith@gmail.com"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/walkerrandolphsmith/laces/issues"
  },
  "homepage": "https://github.com/walkerrandolphsmith/laces#readme",
  "devDependencies": {
    "ava": "^0.23.0",
    "babel-cli": "^6.10.1",
    "babel-core": "^6.10.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-preset-stage-1": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "babel-preset-stage-3": "^6.5.0",
    "express": "^4.16.2"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-3",
      "stage-2",
      "stage-1",
      "stage-0"
    ],
    "plugins": [
      [
        "transform-runtime",
        {
          "polyfill": false,
          "regenerator": true
        }
      ]
    ]
  },
  "ava": {
    "files": [
      "./**/*.test.js"
    ],
    "source": [
      "packages/**/*.js"
    ],
    "match": [],
    "concurrency": 5,
    "failFast": true,
    "failWithoutAssertions": false,
    "tap": true,
    "powerAssert": false,
    "require": [
      "babel-register",
      "babel-polyfill"
    ],
    "babel": "inherit"
  }
}
