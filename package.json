{
  "name": "@quillio/stringy",
  "version": "1.1.0",
  "description": "Common string utilties written in JavaScript",
  "main": "index.js",
  "scripts": {
    "build": "babel ./src --ignore src/**/*.spec.js -d ./dist",
    "test": "ava",
    "docs": "cd ./docs-src && npm run develop",
    "deploy:docs": "cd ./docs-src && npm run deploy"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/walkerrandolphsmith/stringy.git"
  },
  "keywords": [
    "javascript",
    "strings",
    "string",
    "library",
    "lib"
  ],
  "author": "public@quillio.io",
  "contributors": [
    "walkerrandolphsmith@gmail.com"
  ],
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/walkerrandolphsmith/stringy/issues"
  },
  "homepage": "https://github.com/walkerrandolphsmith/stringy#readme",
  "devDependencies": {
    "ava": "^0.22.0",
    "babel-cli": "^6.10.1",
    "babel-core": "^6.10.4",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-preset-stage-1": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "babel-preset-stage-3": "^6.5.0",
    "lerna": "^2.0.0-rc.4",
    "uglify-js": "^3.1.0"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-3",
      "stage-2",
      "stage-1",
      "stage-0"
    ],
    "plugins": [
      ["transform-runtime", {
        "polyfill": false,
        "regenerator": true
      }]
    ]
  },
  "engines": {
    "node": "4.1.1"
  },
  "ava": {
    "files": [
      "packages/**/*.test.js"
    ],
    "source": [
      "packages/**/*.js"
    ],
    "match": [],
    "concurrency": 5,
    "failFast": true,
    "failWithoutAssertions": false,
    "tap": true,
    "powerAssert": false,
    "require": [
      "babel-register",
      "babel-polyfill"
    ],
    "babel": "inherit"
  },
  "dependencies": {
    "babel-plugin-transform-runtime": "^6.23.0"
  }
}
